/*
 * nf-test configuration for nf-index-genome.
 * Test inputs use nf-core/test-datasets (branch: modules), same as nf-core/modules tests.
 */
config {
    testsDir = "tests"
    workDir  = System.getenv("NFT_WORKDIR") ?: ".nf-test"
    configFile = "tests/config/nextflow.config"
    profile = ""
}
